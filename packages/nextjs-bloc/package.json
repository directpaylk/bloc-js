{
  "name": "@bloc-js/nextjs-bloc",
  "version": "1.0.10",
  "description": "A small helper library for using bloc-js with Next.js",
  "author": "Tim Smart <tim.smart@arisechurch.com>",
  "homepage": "https://github.com/tim-smart/bloc-js#readme",
  "license": "ISC",
  "main": "lib/nextjs-bloc.js",
  "module": "lib-es/nextjs-bloc.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "lib-es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tim-smart/bloc-js.git"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "tsc:es": "tsc -m esnext -t esnext --outDir lib-es/",
    "tsc:cjs": "tsc --outDir lib/",
    "tsc": "npm run tsc:es && npm run tsc:cjs",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/tim-smart/bloc-js/issues"
  },
  "peerDependencies": {
    "@bloc-js/bloc": "^1.0",
    "next": "^8.0",
    "react": "^16.8"
  },
  "devDependencies": {
    "@bloc-js/bloc": "file:../bloc",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.18",
    "next": "^8.1.0",
    "react": "^16.8.6"
  }
}
